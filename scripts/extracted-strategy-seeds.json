  {
    "name": "Opening Range Breakout + Patient Candle (Long)",
    "slug": "orb-pc-long",
    "description": "Detects bullish breakout above 15-minute opening range with patient candle confirmation. Requires 5-60 minutes after open, volume above average, and at least 3 confluence factors.",
    "category": "OPTIONS_DAY_TRADE",
    "underlyingScope": "ANY",
    "timeWindow": {
      "start": "09:35",
      "end": "10:30",
      "timezone": "America/New_York"
    },
    "barTimeframe": "5m",
    "entrySide": "LONG",
    "optionsPlayType": "single_leg",
    "conditions": {
      "type": "AND",
      "children": [
        {
          "type": "RULE",
          "rule": { "field": "session.minutesSinceOpen", "op": ">=", "value": 5 }
        },
        {
          "type": "RULE",
          "rule": { "field": "session.minutesSinceOpen", "op": "<=", "value": 60 }
        },
        {
          "type": "RULE",
          "rule": { "field": "price.current", "op": ">", "value": "pattern.orbHigh" }
        },
        {
          "type": "RULE",
          "rule": { "field": "pattern.isPatientCandle", "op": "==", "value": true }
        },
        {
          "type": "RULE",
          "rule": { "field": "pattern.volumeSpike", "op": "==", "value": true }
        }
      ]
    },
    "alertBehavior": {
      "flashWatchlist": true,
      "showNowPlaying": true,
      "notifyDiscord": true,
      "autoOpenTradePlanner": false
    },
    "cooldownMinutes": 15,
    "oncePerSession": true,
    "isCoreLibrary": true,
    "enabled": true
